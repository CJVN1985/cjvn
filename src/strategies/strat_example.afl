// Example strategy using ind_ema (placeholder)
SetBarsRequired(10000, 0);
SetOption("DisableRuinStop", True);
SetPositionSize(10, spsPercentOfEquity);

strat_ema_per = Nz(Param("EMA Period", 20, 2, 200, 1), 20);
strat_ema     = EMA(Close, strat_ema_per);

Buy  = Cross(Close, strat_ema);
Sell = Cross(strat_ema, Close);

PositionScore = ROC(Close, 5);
